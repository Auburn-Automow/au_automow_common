<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find automow_2dnav)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find automow_2dnav)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find automow_2dnav)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find automow_2dnav)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find automow_2dnav)/config/base_local_planner_params.yaml" command="load" />
    <param name="controller_frequency" value="20" />
</node>